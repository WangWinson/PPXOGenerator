{
  "name": "PayPal Checkout Generator",
  "version": "1.0",
  "description": "One stop PayPal Checkout Integration",
  "browser_action": {
    "default_icon": {
      "19": "icon/pp_editor.png"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "declarativeContent",
    "activeTab",
    "tabs",
    "storage",
    "webNavigation",
    "https://www.baidu.com/*",
    "https://www.amazon.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "script/insertBtn.js",
    "script/mod.js",
    "script/parameters.js",
    "script/paypalCodeGen.js"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.amazon.com/*", "https://www.baidu.com/*"],
      "css": ["bulma/bulma.min.css", "prism/prism.css"],
      "js": ["jquery/jquery-3.3.1.min.js", "prism/prism.js"]
    }
  ],
  "manifest_version": 2
}
